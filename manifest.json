{
  "manifest_version": 1,
  "name": "Procrastiantion",
  "version": "0.1",
  "description": "Procrastination extension",

  "browser_action": {
    "default_title": "Procrastination",
    "default_popup": "popup.html",
    "default_icon": "exclamation-mark-red-hi.png",
  },
  "content_scripts": [
      {
        "css": ["contentscripts.css"],
        "js": ["jquery.js", "contentscripts.js"]
      }
    ],
  "content_security_policy": "script-src 'self' https://changethisout.firebaseio.com https://*.firebaseio.com; object-src 'self'",
  "permissions": ["identity", "identity.email", "storage"]
}
