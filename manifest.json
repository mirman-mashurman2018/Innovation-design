
{
  "manifest_version": 2,
  "name": "Procrastination",
  "version": "0.3",
  "description": "Procrastination extension",

  "browser_action": {
    "default_title": "Procrastination",
    "default_popup": "procrastination.html",
    "default_icon": ""
  },
  "content_scripts": [
  {
    "css": ["script.css"],
    "js": ["jquery.js", "contentscript.js"]
  }
],
  "content_security_policy": "script-src 'self' https://mirmindr.firebaseio.com https://*.firebaseio.com; object-src 'self'",
  "permissions": ["identity", "identity.email", "storage"]
}
